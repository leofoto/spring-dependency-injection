<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="twitterService" class="com.reactivestax.services.TwitterService"></bean>
	<bean id="emailService" class="com.reactivestax.services.EmailService"></bean>
	<bean id="smsService" class="com.reactivestax.services.SMSService"></bean>

	<bean id="MyXMLApp" class="com.reactivestax.consumer.MyXMLApplication">
		<property name="service" ref="smsService"></property>
	</bean>
</beans>